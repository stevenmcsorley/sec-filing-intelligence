      "lastName": "User",
      "email": "test@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "password",
          "temporary": false
        }
      ],
      "realmRoles": [
        "basic_free"
      ]
    }
  ],
  "defaultRole": {
    "name": "default-roles-sec-intel",
    "composite": true,
    "clientRole": false,
    "containerId": "sec-intel",
