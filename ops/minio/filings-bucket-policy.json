{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowMinioConsoleAccess",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::filings-raw",
        "arn:aws:s3:::filings-raw/*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:PrincipalType": "User"
        }
      }
    }
  ]
}
